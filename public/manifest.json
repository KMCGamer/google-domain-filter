{
  "manifest_version": 2,
  "name": "Google Search Blacklist",
  "author": "Kyle Carhart",
  "version": "1.0",
  "description": "Filter domains from your Google searches.",
  "permissions": ["storage", "activeTab"],
  "icons": {
    "18": "./icons/18dp.png",
    "48": "./icons/48dp.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["content.js"]
    }
  ]
}
